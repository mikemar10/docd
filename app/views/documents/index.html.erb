<% @documents.each_slice(5) do |documents| %>
<ul class="list-group row col-xs-offset-1">
  <% documents.each do |document| %>
    <li class="list-group-item col-xs-2 document-thumbnail">
      <%= link_to image_tag(document.document.url(:thumb), alt: document.name), document_path(document.id) %>
    </li>
  <% end %>
</ul>
<% end %>
